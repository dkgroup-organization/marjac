<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_move_form_expiry_inherits_prodlots" model="ir.ui.view">
            <field name="name">stock.lot.form.inherits</field>
            <field name="model">stock.lot</field>
             <field name="inherit_id" ref="product_expiry.view_move_form_expiry" />
            <field name="arch" type="xml">

                <xpath expr="//field[@name='removal_date']" position="after">
                    <field name="fabrication_date"/>
                </xpath>
                
            </field>
        </record>    
		
    </data>
</odoo>